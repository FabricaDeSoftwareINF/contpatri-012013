<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.ufg.inf.es.fs.contpatri.model.Analise" table="Analise" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nome" type="string">
            <column name="nome" length="45" not-null="true" />
        </property>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.BemPatrimonial" fetch="select" name="bemPatrimonial">
            <column name="idBemPatrimonial" not-null="true"/>
        </many-to-one>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.Inventario" fetch="select" name="inventario">
            <column name="idInventario" not-null="true"/>
        </many-to-one>
        <property name="TipoSituacaoAnalise" column="tipoSituacaoAnalise">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">
                    br.ufg.inf.es.fs.contpatri.model.Analise.TipoSituacaoAnalise
                </param>
            </type>       
        </property>
    </class>
</hibernate-mapping>
