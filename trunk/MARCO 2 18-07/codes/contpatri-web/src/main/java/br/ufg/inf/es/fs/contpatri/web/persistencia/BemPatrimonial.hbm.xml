<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.ufg.inf.es.fs.contpatri.model.BemPatrimonial" table="BemPatrimonial" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="descricao" type="string">
            <column name="descricao" length="45" not-null="true" />
        </property>
        <property name="dataAquisicao" type="timestamp">
            <column length="19" name="dataAquisicao" not-null="true"/>
        </property>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.Origem" fetch="select" name="Origem">
            <column name="idOrigem" not-null="true"/>
        </many-to-one>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.Gestor" fetch="select" name="Gestor">
            <column name="idGestor" not-null="true"/>
        </many-to-one>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.Agente" fetch="select" name="Agente">
            <column name="idAgente" not-null="true"/>
        </many-to-one>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.Tombamento" fetch="select" name="Tombamento">
            <column name="idTombamento" not-null="true"/>
        </many-to-one>
        <many-to-one class="br.ufg.inf.es.fs.contpatri.model.SubLocal" fetch="select" name="SubLocal">
            <column name="idSubLocal" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
